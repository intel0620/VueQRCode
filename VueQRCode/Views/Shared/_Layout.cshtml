<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">

    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.js"></script>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css" id="theme-styles">

</head>
<body style="background-color:#dfdfdf;" class="">
    <nav class="navbar  " style="background-color: #f0f0f0; ">
        <h2>QRCode 掃描</h2>
        @*<img src="~/Images/logo.png" class="img-fluid d-inline-block align-top " alt="Responsive image">*@
    </nav>
    <br />
    <div class="container">
        @RenderBody()
        <hr />
        <div class="footer" style="font-size: x-small">
            <div class="float-right">

            </div>
            <div>
                <strong>
                    Copyright
                </strong> &copy; @DateTime.Now.Year @*YUEN FOONG PAPER CO.,LTD. 永豐紙業 All rights reserved*@.

            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
